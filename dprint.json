{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 100,
  "typescript": {
    "quoteProps": "asNeeded",
    "arrowFunction.useParentheses": "force",
    "semiColons": "asi"
  },
  "markdown": {
    "lineWidth": 80,
    "textWrap": "always",
    "emphasisKind": "asterisks"
  },
  "includes": ["**.{json,ts,wat,md}"],
  "plugins": [
    "https://plugins.dprint.dev/json-0.17.0.wasm",
    "https://plugins.dprint.dev/markdown-0.15.2.wasm",
    "https://plugins.dprint.dev/typescript-0.80.2.wasm",
    "https://plugins.dprint.dev/tjjfvi/form.wat-v0.1.1.wasm"
  ]
}
